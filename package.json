{
  "name": "@journium/journium-docs",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "NX_WORKSPACE_ROOT_PATH=$PWD nx dev",
    "build": "NX_WORKSPACE_ROOT_PATH=$PWD nx build",
    "start": "NX_WORKSPACE_ROOT_PATH=$PWD nx start",
    "lint": "NX_WORKSPACE_ROOT_PATH=$PWD nx lint",
    "serve": "NX_WORKSPACE_ROOT_PATH=$PWD nx serve",
    "nx": "NX_WORKSPACE_ROOT_PATH=$PWD nx"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.24.2"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core",
      "nx"
    ]
  },
  "devDependencies": {
    "@nx/eslint": "22.5.0",
    "@nx/js": "22.5.0",
    "@nx/next": "22.5.0",
    "@nx/node": "22.5.0",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "1.15.8",
    "@swc/helpers": "^0.5.17",
    "next": "^16.1.6",
    "nx": "22.5.0",
    "typescript": "^5.9.3"
  }
}